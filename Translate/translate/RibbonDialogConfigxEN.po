msgid "Последние файлы:"
msgstr "Latest files:"

msgid "Редактирование"
msgstr "Editing"

msgid "Фильтры"
msgstr "Filters"

msgid "редактировать MIDI ключ"
msgstr "edit MIDI key"

msgid "установить MIDI ключ как.."
msgstr "set MIDI key as..."

msgid "параметры редактора"
msgstr "editor options"

msgid "выбор фильтров просмотра"
msgstr "selection of viewing filters"

msgid "сортировка"
msgstr "sorting"

msgid "&Меню"
msgstr "&Menu"

msgid "Меню приложения"
msgstr "Application menu"

msgid "&Быстрый доступ"
msgstr "&Fast access"

msgid "Панель быстрого доступа"
msgstr "Quick Access Toolbar"

msgid "&Сохранить"
msgstr "&Save"

msgid "Сохранить и применить"
msgstr "Save and Apply"

msgid "Сохранить в загруженный ранее файл и обновить текущую конфигурацию."
msgstr "Save to the previously downloaded file and update the current configuration."

msgid "&Экспорт"
msgstr "&Export"

msgid "Экспорт в новый файл"
msgstr "Export to a new file"

msgid "Экспортировать отредактированную конфигурацию в новый файл."
msgstr "Export the edited configuration to a new file."

msgid "&Импорт"
msgstr "&Import"

msgid "Импорт из файла"
msgstr "Import from file"

msgid "Импортировать конфигурацию из файла."
msgstr "Import configuration from file."

msgid "&Выход"
msgstr "&Exit"

msgid "Выход из редактора"
msgstr "Exit editor"

msgid "Выход из редактора конфигурации MIDI ключей, если изменения не сохранены, они будут утеряны."
msgstr "Exits the MIDI Key Configuration Editor; if changes are not saved, they will be lost."

msgid "&Новый MIDI ключ"
msgstr "&New MIDI key"

msgid "Новый MIDI ключ"
msgstr "New MIDI key"

msgid "Создание нового MIDI ключа."
msgstr "Creating a new MIDI key."

msgid "&Копировать"
msgstr "&Copy"

msgid "Копировать MIDI ключ"
msgstr "Copy MIDI key"

msgid "Скопировать выбранную запись MIDI ключа."
msgstr "Copy the selected MIDI key entry."

msgid "&Вставить"
msgstr "&Insert"

msgid "Вставить ключ"
msgstr "Insert key"

msgid "Вставить скопированный MIDI ключ."
msgstr "Paste the copied MIDI key."

msgid "&Удалить"
msgstr "&Delete"

msgid "Удалить ключ"
msgstr "Delete key"

msgid "Удалить выбранную запись MIDI ключа."
msgstr "Delete the selected MIDI key entry."

msgid "Умный дом"
msgstr "Smart House"

msgid "Установить группу как «Умный дом»."
msgstr "Set the group as “Smart Home”."

msgid "Назначить выбранный MIDI ключ к группе «Умный дом»."
msgstr "Assign the selected MIDI key to the “Smart Home” group."

msgid "Мультимедиа кнопки"
msgstr "Multimedia buttons"

msgid "Установить группу как «Мультимедиа кнопки»"
msgstr "Set the group as \"Multimedia Buttons\""

msgid "Назначить выбранный MIDI ключ к группе «Мультимедиа кнопки»."
msgstr "Assign the selected MIDI key to the “Multimedia Buttons” group."

msgid "Аудио микшер"
msgstr "Audio mixer"

msgid "Установить группу как «Аудио микшер»"
msgstr "Set group as \"Audio Mixer\""

msgid "Назначить выбранный MIDI ключ к группе «Аудио микшер»."
msgstr "Assign the selected MIDI key to the “Audio Mixer” group."

msgid "Читать MIDI код"
msgstr "Read MIDI code"

msgid "Прочесть MIDI код контрола"
msgstr "Read MIDI control code"

msgid "Чтение MIDI кода из поверхности контроля или MIDI клавиатуры."
msgstr "Read MIDI code from a control surface or MIDI keyboard."

msgid "Только цифровой ввод"
msgstr "Digital input only"

msgid "Использовать только цифровой ввод"
msgstr "Use digital input only"

msgid "Не использовать меню выбора значений, использовать только цифровой ввод."
msgstr "Do not use the value selection menu, use digital input only."

msgid "MIDI панель управления"
msgstr "MIDI control panel"

msgid "Рекомендуемые настройки"
msgstr "Recommended Settings"

msgid "Рекомендуемые настройки панели управления MIDI, совместимые с элементами управления Adobe Premiere Pro."
msgstr "Recommended MIDI control panel settings compatible with Adobe Premiere Pro controls."

msgid "Встроенные фильтры"
msgstr "Built-in filters"

msgid "Ключ"
msgstr "Key"

msgid "Фильтровать по MIDI ключу"
msgstr "Filter by MIDI key"

msgid "Показывать только совпадения в группе MIDI ключей."
msgstr "Show only matches within a MIDI key group."

msgid "Сцена"
msgstr "Scene"

msgid "Фильтровать по MIDI сцене"
msgstr "Filter by MIDI scene"

msgid "Показывать только совпадения в группе MIDI сцены."
msgstr "Show only matches in the MIDI scene group."

msgid "Назначение 1"
msgstr "Purpose 1"

msgid "Фильтровать по назначению 1"
msgstr "Filter by destination 1"

msgid "Показывать только совпадения в группе по целевому назначению 1."
msgstr "Show only matches in the target group 1."

msgid "Назначение 2"
msgstr "Purpose 2"

msgid "Фильтровать по назначению 2"
msgstr "Filter by destination 2"

msgid "Показывать только совпадения в группе по целевому назначению 2."
msgstr "Show only matches in the target group 2."

msgid "Очистить фильтры"
msgstr "Clear filters"

msgid "Очистка фильтров"
msgstr "Cleaning filters"

msgid "Полная очистка фильтров, будут показаны все записи."
msgstr "Completely clear filters, all entries will be shown."

msgid "Установить фильтр"
msgstr "Set filter"

msgid "Установка фильтра"
msgstr "Filter installation"

msgid "Установка фильтра показывает все недействительные элементы."
msgstr "Setting the filter shows all invalid items."

msgid "Применить фильтр"
msgstr "Apply filter"

msgid "Применение фильтра"
msgstr "Applying a filter"

msgid "Применение фильтра показывает выбранные элементы."
msgstr "Applying a filter shows the selected items."

msgid "Отменить фильтр"
msgstr "Cancel filter"

msgid "Отмена фильтра"
msgstr "Cancel filter"

msgid "Отмена фильтра покажет все элементы."
msgstr "Canceling the filter will show all items."

msgid "Показать «Мультимедиа кнопки»"
msgstr "Show \"Multimedia Buttons\""

msgid "Фильтровать по группе «Мультимедиа кнопки»"
msgstr "Filter by “Multimedia buttons” group"

msgid "Применение фильтра покажет членов группы «Мультимедиа кнопки»."
msgstr "Applying a filter will show the members of the Multimedia Buttons group."

msgid "Показать «Аудио микшер»"
msgstr "Show \"Audio Mixer\""

msgid "Фильтровать по группе «Аудио микшер»"
msgstr "Filter by “Audio Mixer” group"

msgid "Применение фильтра покажет членов группы «Аудио микшер»."
msgstr "Applying a filter will show the members of the Audio Mixer group."

msgid "Показать «Умный дом»"
msgstr "Show \"Smart Home\""

msgid "Фильтровать по группе «Умный дом»"
msgstr "Filter by “Smart Home” group"

msgid "Применение фильтра покажет членов группы «Умный дом»."
msgstr "Applying a filter will show the members of the Smart Home group."

msgid "Тип фильтра"
msgstr "Filter type"

msgid "Установка типа применяемых фильтров"
msgstr "Setting the type of filters to apply"

msgid "Типы применяемых фильтров имеют два логических состояния: «И», «ИЛИ»."
msgstr "The types of filters used have two logical states: “AND”, “OR”."

msgid "Выбор типа фильтрации"
msgstr "Selecting a filter type"

msgid "Показывать любые совпадения"
msgstr "Show any matches"

msgid "Показывать записи с любым совпадением"
msgstr "Show entries with any match"

msgid "Фильтр имеет состояние логического «И» и показывает любые совпадения."
msgstr "The filter has a logical AND state and shows any matches."

msgid "Показывать только полные совпадения"
msgstr "Show only complete matches"

msgid "Показывать записи только с полным совпадением"
msgstr "Show records with exact matches only"

msgid "Фильтр имеет состояние логического «ИЛИ» и показывает только полные совпадения."
msgstr "The filter has a logical OR state and shows only complete matches."

msgid "Авто отправка данных в фильтре"
msgstr "Automatic sending of data in the filter"

msgid "Авто отправка вводимых значений в фильтре"
msgstr "Automatic sending of entered values ​​in the filter"

msgid "Автоматически отправляет вводимые значения во встроенном фильтре."
msgstr "Automatically submits input values ​​in a built-in filter."

msgid "Показать сортировку"
msgstr "Show sorting"

msgid "по ключу"
msgstr "by key"

msgid "Сортировка по возрастанию и MIDI ключу."
msgstr "Sort by ascending and MIDI key."

msgid "по сцене"
msgstr "around the stage"

msgid "Сортировка по возрастанию и MIDI сцене."
msgstr "Sorting by ascending and MIDI scene."

msgid "по назначению 1"
msgstr "by appointment 1"

msgid "Сортировка по возрастанию и MIDI назначению 1."
msgstr "Sorting by ascending and MIDI destination 1."

msgid "по назначению 2"
msgstr "by appointment 2"

msgid "Сортировка по возрастанию и MIDI назначению 2."
msgstr "Sorting by ascending and MIDI destination 2."

msgid "Сортировка по убыванию и MIDI ключу."
msgstr "Sort by descending and MIDI key."

msgid "Сортировка по убыванию и MIDI сцене."
msgstr "Sorting by descending order and MIDI scene."

msgid "Сортировка по убыванию и MIDI назначению 1."
msgstr "Sorting by descending order and MIDI destination 1."

msgid "Сортировка по убыванию и MIDI назначению 2."
msgstr "Sorting by descending order and MIDI destination 2."
